
<link rel="stylesheet" type="text/css" href="/css/style.css">
<link rel="stylesheet" type="text/css" href="/css/employeeList.css">


<strong>Employees with Last Name {{name}}</strong>
<hr />

<table>
  <tr>
    <th>Id</th>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
    {{#each employees}}
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.firstName}}</td>
        <td>{{this.lastName}}</td>    
      </tr>  
    {{/each}}
</table>
