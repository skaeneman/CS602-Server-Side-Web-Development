
<link rel="stylesheet" href="../css/style.css">
<link rel="stylesheet" href="../css/employeeList.css">

<div class="empList">
<strong>Employees with Last Name {{lastName}}</strong>
<hr />

<table>
  <tr>
    <th>Id</th>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
    {{#each employees}}
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.firstName}}</td>
        <td>{{this.lastName}}</td>    
      </tr>  
    {{/each}}
</table>
</div>